<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ispitv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IspitV2">
        <activity android:name=".EpisodesFilterResult"></activity>
        <activity android:name=".EpisodesFilterActivity" />
        <activity android:name=".LocationsFilterResult" />
        <activity android:name=".LocationsFilterActivity" />
        <activity android:name=".CharactersFilterResult" />
        <activity android:name=".CharactersFilterActivity" />
        <activity android:name=".EpisodesActivity" />
        <activity android:name=".LocationsActivity" />
        <activity android:name=".CharactersActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>